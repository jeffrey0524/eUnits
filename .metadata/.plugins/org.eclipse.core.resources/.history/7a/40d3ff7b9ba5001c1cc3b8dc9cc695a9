
public class squaredancer extends Dancer {

	public squaredancer() {
		super();
		// TODO Auto-generated constructor stub
	}

	public squaredancer(int x, int y, int dir, int beep) {
		super(x, y, dir, beep);
		// TODO Auto-generated constructor stub
	}
	@Override
	public void danceStep() {
		// TODO Auto-generated method stub
		move();
		turnLeft();
		move();
		turnleft();
		move();
		turnleft();
		move();
		turnleft();
	}

}
