package Final;

import java.awt.Color;
import java.awt.Graphics;

public class panaladd {
	public int hits =0; 
	
	public void startimg(Graphics myBuffer,int FRAME)
    {
		myBuffer.setColor(Color.blue);
  	    myBuffer.fillRect(0,0,FRAME,FRAME);
  	    myBuffer.setColor(Color.blue);
  	    myBuffer.fillRect(0,0,FRAME,FRAME);
    }
	public void hitsup()
    {
		hits+=1;
    }
	
}
